{
	"trailingComma": "none",
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrderCaseInsensitive": true,
	"htmlWhitespaceSensitivity": "ignore",
	"embeddedLanguageFormatting": "auto",
	"importOrderParserPlugins": [
		"classProperties",
		"decorators-legacy",
		"typescript"
	]
}
